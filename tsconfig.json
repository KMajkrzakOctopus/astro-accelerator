{
  "extends": "astro/tsconfigs/strict",
  "types": ["astro/client"],
  "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@layouts/*": ["src/layouts/*"],
        "@head/*": ["src/components/Head/*"],
        "@content/*": ["src/components/Content/*"],
        "@language/*": ["src/components/Language/*"],
        "@paging/*": ["src/components/Paging/*"],
        "@utilities/*": ["src/utilities/*"],
      }
  }
}